{
    "manifest_version":2,
    "name": "Web_Scraper",
    "version" : "1.0",
    "description" : "LinkedIn_WebScraper",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js":["content.js"]
        }
    ],
    "permissions": [
        "downloads"
      ],
    "browser_action":{
        "default_icon":"icon.png"
    },
    "background":{
        "scripts":["background.js"]
    }
}